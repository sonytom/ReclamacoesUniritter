<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.tom.reclamacao" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"

        android:label="@string/app_name"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.example.tom.reclamacao.ReclamacaoActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.tom.reclamacao.LoginActivity"
            android:label="@string/title_activity_main_activity2" >
        </activity>
        <activity
            android:name="com.example.tom.reclamacao.ListareclamaActivity"
            android:label="@string/title_activity_listareclamacoes" >
        </activity>
        <activity
            android:name="com.example.tom.reclamacao.CadastroActivity"
            android:label="@string/title_activity_cadastro" >
        </activity>
    </application>

</manifest>
